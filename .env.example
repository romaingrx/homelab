# Tailscale OAuth client credentials (scope: devices:read)
# Create at: https://login.tailscale.com/admin/settings/oauth
TS_OAUTH_CLIENT_ID=
TS_OAUTH_CLIENT_SECRET=

# Cloudflare API token (scope: Zone.DNS:Edit on romaingrx.com)
# Used only for Let's Encrypt DNS-01 cert validation
# Create at: https://dash.cloudflare.com/profile/api-tokens
CF_API_TOKEN=

# acme.sh email for Let's Encrypt registration
ACME_EMAIL=
